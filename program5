#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int a[n];
    int odd = 0, even = 0;
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        if (a[i] % 2 == 0) {
            even++;
        } else {
            odd++;
        }
    }
    printf("Odd = %d\n", odd);
    printf("Even = %d\n", even);

    return 0;
